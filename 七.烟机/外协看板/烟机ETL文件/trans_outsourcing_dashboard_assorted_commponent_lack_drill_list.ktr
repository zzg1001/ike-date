    <sql>with a as (
select 
case when b.LIFNR like '0%' then SUBSTRING(b.LIFNR, PATINDEX('%[^0]%', b.LIFNR), len(b.LIFNR)-PATINDEX('%[^0]%', b.LIFNR)+1)
else b.LIFNR end as LIFNR_new,
SORTL factory
from ODS_HANA.dbo.LFA1 b
group by LIFNR,SORTL
)
select v.factory_code,a.factory LIFNR_new,v.factory from Outsourcing_Dashboard.dbo.outsourcing_mapping_vender v 
left join a on a.LIFNR_new = v.factory_code </sql>
