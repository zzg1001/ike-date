notice_line_num,  #单据行号
node_name,  #节点名
start_time,
close_time,
sub_send_time,  #送出时间
sub_back_time,  #送回时间
order_num,  #订单号
case when `status`=0 then '初始化'
when `status`=1 then '开始'
when `status`=2 then '异常'
when `status`=3 then '已结束' end as node_status,
trim(leading '0' from order_line_num) as order_line_num,  #订单行号
datediff(close_time,start_time) as fetch_holdup_date,
datediff(sub_back_time,sub_send_time) as fetch_return_holdup_date
from t_logistical_node_pur_migo
</sql>
