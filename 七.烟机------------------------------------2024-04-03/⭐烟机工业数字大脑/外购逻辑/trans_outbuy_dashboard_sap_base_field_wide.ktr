    <connection>ODS_SRM</connection>
    <sql>select 
distinct 
a.ORDER_NUM,
a.ORDER_ITEM_NUM,
a.ITEM_CD,
a.ITEM_DESC,
a.ORDER_QTY,
--b.GR_qty,
b.status,
--b.NODE_STATUS,
--c.CURRENT_STATUS,
b.DELIVERY_QTY ACTUAL_DELIVERY_QTY,
d.DN_HD_NUM as notice_num,
c.line_num as notice_line,
e.REQUIRE_DATE,
f.CREATED_TS,
isnull(c.unqualified_num,0) unqualified_num,
case when d.DN_HD_NUM like '%Q%'  then '急件'
else '一般零件' end as DN_HD_NUM
from srm_poc_order_item a
join srm_poc_order_item_extra b on a.ID=b.ORDER_ITEM_ID
--join (select a.* from srm_poc_delivery_note_item a
join srm_poc_delivery_note_item c on a.ID=c.ORDER_ITEM_ID and c.current_status='收货节点/结束指令'
--join (select order_num,order_item_num,max(version) as version from srm_poc_delivery_note_item --where order_num = '4500055245' and order_item_num = '10'
--group by order_num,order_item_num) b on a.order_num=b.order_num and a.order_item_num=b.order_item_num
--where a.version=b.version) c on a.ID=c.ORDER_ITEM_ID
join srm_poc_delivery_note_hd d on c.DN_HD_ID=d.ID     --装运单行表
left join (select ORDER_NUM,order_item_num,max(REQUIRE_DATE) as REQUIRE_DATE from srm_poc_order_item group by ORDER_NUM,ORDER_ITEM_NUM) e on a.order_num=e.order_num and a.ORDER_ITEM_NUM=e.ORDER_ITEM_NUM
left join (select ORDER_NUM,PURCHASE_GRP,min(CREATED_TS) as CREATED_TS from srm_poc_order_hd group by ORDER_NUM,PURCHASE_GRP) f on c.order_num=f.order_num
where  a.PLANT='2000' and a.DELETE_FLAG=0 and c.DELETE_FLAG=0  and d.DELETE_FLAG=0  
and f.PURCHASE_GRP  in
(
'201'
,'205'
,'207'
,'208'
,'210'
,'212'
,'214'
,'215'
,'211'
,'216'

)
</sql>
