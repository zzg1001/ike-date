          SQLTask:SqlStatementSource="update [dbo].[ppl_qad_base_field_wide] set sbu=b.[PL SBU]&#xA;  from [Cost_Deployment_DB].[dbo].[cc_mapping] b&#xA;  where upper([dbo].[ppl_qad_base_field_wide].cost_center)=b.[cost center] collate SQL_Latin1_General_CP1_CI_AS &#xA;  and upper([dbo].[ppl_qad_base_field_wide].prh_domain)=b.domain collate SQL_Latin1_General_CP1_CI_AS;&#xA;&#xA;  update [Cost_Deployment_DB].[dbo].[ppl_qad_base_field_wide]  set sbu='LS' where prh_domain='CN75';&#xA;&#xA;  update [dbo].[ppl_qad_base_field_wide] set sbu=b.[PL SBU]&#xA;  from [Cost_Deployment_DB].[dbo].[cc_mapping] b&#xA;  where upper([dbo].[ppl_qad_base_field_wide].cost_center)=b.[cost center] collate SQL_Latin1_General_CP1_CI_AS &#xA;  and [dbo].[ppl_qad_base_field_wide].prh_domain='CN7T' and b.domain='CN78' and sbu is null;&#xA;&#xA;update  [dbo].[ppl_qad_base_field_wide] set sbu=case when sbu='FF' or sbu='TS' then 'CMP'&#xA;when sbu='PS'  then 'LS'&#xA;when sbu is null then 'Shared' &#xA;else sbu end;&#xA;&#xA;update ppl_qad_base_field_wide set prh_part=upper(trim(prh_part));&#xA;update ppl_qad_base_field_wide set prh_vend=upper(prh_vend);&#xA;&#xA;update ppl_qad_base_field_wide set categoryowner=b.[categoryowner] &#xA;from [dbo].[ppl_dim_category] b&#xA;where ppl_qad_base_field_wide.[Category Group]=b.[Category_group] &#xA;and ppl_qad_base_field_wide.category=b.category;&#xA;&#xA;update [dbo].[ppl_qad_base_field_wide] set prh_pur_cost_eur=prh_pur_cost*b.CNY_bud_rate&#xA;  from [ppl_currency_exchange_rate] b&#xA;  where b.[Year]=year(getdate()) and [ppl_qad_base_field_wide].prh_curr=b.Currency;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
