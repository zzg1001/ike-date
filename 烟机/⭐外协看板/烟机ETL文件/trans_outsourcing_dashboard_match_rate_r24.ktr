    <connection>ODS_HANA</connection>
    <sql>with m as ----投产决策报表中有的物料号，并且能够匹配供方
(
select 
material_code1,
--primary_vendor,
v.factory,
SUBSTRING(primary_vendor, 1, CHARINDEX('/', primary_vendor)-1) as factory_code
from Purchase_Dashboard.dbo.report_production_decision d 
join Outsourcing_Dashboard.dbo.outsourcing_mapping_vender v on SUBSTRING(primary_vendor, 1, CHARINDEX('/', primary_vendor)-1) = v.factory_code
where material_code1 is not null
)
, n as ( ---通过询价库找到所需要的物料号,WBS号，wbs号的创建日期，一个wbs号可能对应多个物料号
select 
case when m.MATNR like '0%' then SUBSTRING(m.MATNR, PATINDEX('%[^0]%', m.MATNR), len(m.MATNR)-PATINDEX('%[^0]%', m.MATNR)+1)
else m.MATNR end as MATNR,
--a.AUFNR,
--a.PROJN,
c.POSID_EDIT PSPEL,
c.ERDAT
from RESB m
join AFPO a on m.AUFNR = a.AUFNR 
join PRPS c ON a.PROJN = c.PSPNR ----通过关联取到WBS号
join (select MATNR from  MARC where EKGRP='203' group by MATNR)  b on m.MATNR=b.MATNR
where m.WERKS='2000'  and (m.DUMPS&lt;&gt;'X' or m.DUMPS is null)
and a.DAUAT='ZP12'
)
select m.factory_code,m.material_code1, 
m.factory, b.PSPEL
,b.ERDAT
from  m 
join (select * from n group by MATNR,PSPEL,ERDAT) b on m.material_code1 = b.MATNR
</sql>
