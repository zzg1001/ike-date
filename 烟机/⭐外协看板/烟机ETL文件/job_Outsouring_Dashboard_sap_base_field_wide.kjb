      <sql>

with tmp as(
select EBELN,EBELP,max(close_time) as close_time,sum(unqualified_num) as unqualified_num from sap_base_field_wide_temp group by EBELN,EBELP
)
update sap_base_field_wide_temp set close_time=b.close_time,unqualified_num= b.unqualified_num
from tmp b 
where sap_base_field_wide_temp.EBELN=b.EBELN and sap_base_field_wide_temp.EBELP=b.EBELP;

truncate table sap_base_field_wide;

insert into sap_base_field_wide
select distinct * from sap_base_field_wide_temp;

update sap_base_field_wide set is_qualified = case when unqualified_num>0
then '不合格'
else '合格' end; 
--订单数据=收货数：订单已完成
update sap_base_field_wide set finish_or_not=case when MENGE=WEMNG then '完成'
when MENGE!=WEMNG and srm_order_qty=ACTUAL_DELIVERY_QTY then '完成'
--when MENGE>WEMNG and (srm_order_qty is null or ACTUAL_DELIVERY_QTY is null) then null
else '未完成' end; 

--已完成订单行，节点状态是'点收节点/结束指令'，并且有点收结束时间，取订单行对应的最新点收时间做完订单行完成时间
--with tmp as(
--select  EBELN,EBELP,max(RWQUIRE_DATE) as RWQUIRE_DATE from sap_base_field_wide
--where finish_or_not='完成' and RWQUIRE_DATE is not null and CURRENT_STATUS='点收节点/结束指令' 
--group by EBELN,EBELP,RWQUIRE_DATE
--)
--update sap_base_field_wide set completion_date=b.RWQUIRE_DATE
--from tmp b
--where sap_base_field_wide.EBELN=b.EBELN and sap_base_field_wide.EBELP=b.EBELP;

with tmp as(
select EBELN,EBELP,max(close_time) as close_time from sap_base_field_wide group by EBELN,EBELP
)
update sap_base_field_wide set completion_date=case when MENGE=WEMNG and ACTUAL_DELIVERY_QTY=srm_order_qty then b.close_time
when MENGE>WEMNG and ACTUAL_DELIVERY_QTY=srm_order_qty then b.close_time
when MENGE=WEMNG and ACTUAL_DELIVERY_QTY>srm_order_qty then BUDAT
when MENGE>WEMNG and ACTUAL_DELIVERY_QTY>srm_order_qty then null end
from tmp b 
where sap_base_field_wide.EBELN=b.EBELN and sap_base_field_wide.EBELP=b.EBELP;

update sap_base_field_wide set completion_date=case when completion_date is not null then completion_date
when completion_date is null then BUDAT end 
where finish_or_not='完成';

update sap_base_field_wide set overdue_or_not= case when finish_or_not='完成' and CONVERT(varchar, completion_date, 112)&lt;=EINDT then '按时'
when finish_or_not='完成' and CONVERT(varchar, completion_date, 112)>EINDT then '逾期' 
when finish_or_not='未完成' and CONVERT(varchar, GETDATE(), 112)&lt;=EINDT then '按时' 
when finish_or_not='未完成' and CONVERT(varchar, GETDATE(), 112)>EINDT then '逾期' end;

update sap_base_field_wide set order_num_overdue_finish_days=DATEDIFF(day, EINDT, completion_date)
where overdue_or_not='逾期' and finish_or_not='完成';

update sap_base_field_wide set order_num_overdue_days=case when overdue_or_not='逾期' and completion_date is not null then DATEDIFF(day, EINDT, completion_date)
when overdue_or_not='逾期' and completion_date is null then DATEDIFF(day, EINDT, GETDATE()) end;

delete from sap_base_field_wide where factory='新场' and ROUND(unit_price,0)=1;</sql>
