    <connection>ODS_HANA</connection>
    <sql>SELECT 
	a.AUFNR as assemble_order_no,
	c.WERKS as plant_code,
	c.AUART as order_type_code,
	e.TXT as order_type_desc,
	SUBSTRING(f.POSID,1,1)+ '-' + RTRIM(LTRIM(SUBSTRING(f.POSID,2,50))) as wbs_code,
	f.POST1 as wbs_desc,
	b.MATNR as material_code,
	d.MAKTX as material_desc,
	CAST(CASE WHEN a.GLTRP = '00000000' THEN '9999-12-31' ELSE a.GLTRP END  as DATE) as schedule_date,
	CAST(a.FTRMI as DATE) as release_date,
	CASE WHEN CAST(b.LTRMI as int) = 0 THEN NULL ELSE CAST(b.LTRMI as DATE) END as delivery_date,
	a.GAMNG as order_quantity,
	b.WEMNG as delivery_quantity,
	g.ltext as zp_ltxt
FROM
	ODS_HANA.dbo.AFKO a
LEFT OUTER JOIN
	ODS_HANA.dbo.AFPO b ON a.MANDT = b.MANDT AND a.AUFNR = b.AUFNR
LEFT OUTER JOIN
	ODS_HANA.dbo.AUFK c ON a.MANDT = c.MANDT AND a.AUFNR = c.AUFNR
LEFT OUTER JOIN
	ODS_HANA.dbo.MAKT d ON a.MANDT = d.MANDT AND b.MATNR = d.MATNR
LEFT OUTER JOIN
	ODS_HANA.dbo.T003P e ON a.MANDT = e.CLIENT AND c.AUART = e.AUART
LEFT OUTER JOIN
	ODS_HANA.dbo.PRPS f ON a.MANDT = f.MANDT AND b.PROJN = f.PSPNR
LEFT OUTER JOIN
	ODS_HANA.dbo.LTXT g ON a.AUFNR = g.order_no
WHERE a.MANDT = '520' AND c.AUART LIKE 'ZP%' AND a.FTRMI &lt;&gt; '00000000'
AND c.AUART IN ('ZP10','ZP11','ZP12', 'ZP18', 'ZP19', 'ZP20', 'ZP21','ZP13') AND c.WERKS = '2000'
</sql>
